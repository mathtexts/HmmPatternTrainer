command		(\w*звон\w+)|(набрать\w+)|(набер\w+)|(набир\w+)|(покаж\w+)|(откр\w+)|(отправ\w+)|(перешл\w+)|(пересл\w+)|(телефон\w*)|(номер\w*)|(баланс)|(письмо)|(смс)|(sms)
name		(леш\w*)|(ван\w*)|(саш\w*)|(наст\w*)|(даш\w*)|(маш\w*)|(ан\w*)|(пет\w*)|(дим\w*)|(оля\w*)|(андр\w*)|(антон\w*)
tel			(\+?[\d\s]+)
email		([^S]+@[^S]+)
url			((\w+.)+(ru|com|org|net|su))
time		(\d+)